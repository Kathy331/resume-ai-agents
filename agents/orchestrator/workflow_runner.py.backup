                }
            
            # Limit to max_interviews
            interviews_to_process = non_prepped_interviews[:max_interviews]
            print(f"üéØ Found {len(non_prepped_interviews)} non-prepped interviews, processing {len(interviews_to_process)}")
            
            # Phase 2: Enhanced Research with 3-Phase Validation
            print(f"\nüî¨ Phase 2: Enhanced Research with 3-Phase Validation")
            research_contexts = []
